class Solution:
    from itertools import product
    def letterCombinations(self, digits: str) -> List[str]:
        let={"2":"abc","3":"def","4":"ghi","5":"jkl","6":"mno","7":"pqrs","8":"tuv","9":"wxyz"}
        if len(digits)==0:return []
        elif len(digits)==1: return list(map("".join,list(product(let[digits[0]]))))
        elif len(digits)==2: return list(map("".join,list(product(let[digits[0]],let[digits[1]]))))
        elif len(digits)==3: return list(map("".join,list(product(let[digits[0]],let[digits[1]],let[digits[2]]))))
        elif len(digits)==4: return list(map("".join,list(product(let[digits[0]],let[digits[1]],let[digits[2]],let[digits[3]]))))
                                
                                         

        
