from itertools import permutations as p
class Solution:
    def rangeSum(self, nums: List[int], n: int, left: int, right: int) -> int:
        if not nums:return 0
        l=[]
        for i in range(n):
            for j in range(i+1,n+1):
                l.append(sum(nums[i:j]))
        l.sort()
        print(l)
        s=sum(l[left-1:right])
        m=(10**9+7)
        if s>=m:return s%m
        else :return s
